{{#hasctasection}}
<div class="cta-section">
	<div class="container-fluid text-center">
		<div class="cta-section-inner">
			{{#ctatitle}}
			<h2 class="cta-title">{{{ctatitle}}}</h2>
			{{/ctatitle}}
			{{#ctacontent}}
			<div class="cta-content">
				{{{ctacontent}}}
			</div><!--//cta-content-->
			{{/ctacontent}}
			
			{{#hasctadatabox}}
			<div class="cta-data-box mb-3">
				<div class="row justify-content-center">
					{{#ctadataitems}}
					<div class="item col-6 col-lg-3">
						<div class="item-inner p-3 text-white">
							{{#ctadataitemtitle}}<div class="data-title">{{{ctadataitemtitle}}}</div>{{/ctadataitemtitle}}
							{{#ctadataitemmeta}}<div class="data-meta">{{{ctadataitemmeta}}}</div>{{/ctadataitemmeta}}
						</div>
					</div><!--//item-->		
					{{/ctadataitems}}
								
				</div><!--//row-->
			</div><!--//cta-data-box-->
			{{/hasctadatabox}}
			
			{{#hasctabutton}}
			<a class="btn btn-cta theme-btn-secondary" href="{{{ctaurl}}}" {{#urlopennew}}target="_blank"{{/urlopennew}}>{{{ctabutton}}}</a>
			{{/hasctabutton}}
		</div><!--//cta-section-inner-->
	</div><!--//container-fluid-->
</div><!--//cta-section-->
{{/hasctasection}}